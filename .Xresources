! -----------------------------------
Xft.dpi: 102
!Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault


! ===== urxvt =====
URxvt.scrollBar: off
URxvt.internalBorder:     22
URxvt.externalBorder:      0

URxvt.foreground :white
!!URxvt.tintColor : yellow
urxvt*blurRadius: 10
URxvt.letterSpace:         -1


! fonts
URxvt*letterSpace: 	    		-2
URxvt*font:                 	xft:Monospace:regular:size=12
URxvt*imFont:               	xft:Monospace:regular:size=12
URxvt*boldFont:             	xft:Monospace:bold:size=12
URxvt*italicFont:           	xft:Monospace:italic:size=12
URxvt*boldItalicFont:     		xft:Monospace:regular:size=12

URxvt.transparent : 0.7
URxvt.shading : 30
URxvt*fading:              30


! perl extensions
URxvt*perl-ext-common:  default,clipboard,url-select,keyboard-select,resize-font
!URxvt*perl-ext-common:  default,clipboard,url-select,keyboard-select,tabbed

! tabbed
URxvt*tabbed.tabbar-fg: 4
URxvt*tabbed.tabbar-bg: 16
URxvt*tabbed.tab-fg:    15
URxvt*tabbed.tab-bg:    4
URxvt*buffered:         false
URxvt*keysym.Control-Shift-C: perl:clipboard:copy
URxvt*keysym.Control-Shift-V: perl:clipboard:paste

!
 URxvt.keysym.F11:          perl:fullscreen:switch
!
 URxvt.Resize-font.smaller: esc
 URxvt.Resize-font.bigger:  S-Up
!

 URxvt.url-launcher:        /usr/bin/chromium
 URxvt.keysym.M-Delete:     perl:matcher:list
 URxvt.colorUL:             #006699


URxvt.cursorBlink : 1
URxvt.cursorUnderline : 1
URxvt.cursorColor : cyan
URxvt*saveLines: 60000
URxvt.background: #1E262E




